@page
@model IndexModel

@{
    ViewData["Title"] = "Northwind-In-Chart";
}
<div class="container-fluid mx-auto" style="max-width: 75%;">
    <div class="row mt-4">
        @* TotalSum *@
        <div class="col-md-3 mb-2">
            <div class="d-flex flex-row widget summary totalSum">
                <div class="d-flex flex-column justify-content-center p-2">
                    <i class="bi-currency-euro fs-1 text-info"></i>
                </div>
                <div class="flex-column m-auto">
                    <span class="lead">Category sales</span>
                    <div class="vrednost">@ViewData["Total"] <i class="bi-arrow-up text-success"></i></div>
                </div>
            </div>
        </div>

        @* TotalQuantity *@
        <div class="col-md-4 mb-2">
            <div class="d-flex flex-row widget summary totalQuantity">
                <div class="d-flex flex-column justify-content-center p-2">
                    <i class="bi-currency-euro fs-1 text-danger"></i>
                </div>
                <div class="flex-column m-auto">
                    <span class="lead">Total Quantity</span>
                    <div class="vrednost">@ViewBag.TotalQuantity </div>
                </div>
            </div>
        </div>

        @* Revenue *@
        <div class="col-md-4 mb-2">
            <div class="d-flex flex-row widget summary revenue">
                <div class="d-flex flex-column justify-content-center p-2">
                    <i class="bi-currency-euro fs-1 text-success"></i>
                </div>
                <div class="flex-column m-auto">
                    <span class="lead">Revenue</span>
                    <div class="vrednost">@ViewBag.Revenue</div>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="container-fluid mx-auto" style="max-width: 75%;">
    <div class="">
        <div class="row mx-auto">
            <div class="text-center">
                <div class="row">                
                    <div class="chart-container">
                        <canvas id="multiline" class="my-1"></canvas>
                    </div>          
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row mt-4">
    <div class="col-md-4 mb-2">
        <div class="d-flex flex-row summary totalSum">
            <div class="flex-column m-auto">
                <span class="lead"></span>
                <canvas id="CategorySales97" class="vrednost"></canvas>
            </div>
        </div>
    </div>


    <div class="col-md-4 mb-2">
        <div class="d-flex flex-row summary totalSum">
            <div class="flex-column m-auto">
                <span class="lead"></span>
                <canvas id="rale" class="vrednost"></canvas>
            </div>
        </div>
    </div>
</div>

@section Scripts {

    <script type="text/javascript">

        // Category sales chart
            const js = '@Html.Raw(ViewData["CategorySales97"])';
            const val = JSON.parse(js);
            const ct = document.getElementById('CategorySales97');
        new Chart(ct, {
          type: 'doughnut',
          data: {
            labels: [
                val[0].CategoryName,
                val[1].CategoryName,
                val[2].CategoryName,
                val[3].CategoryName,
            ],
            datasets: [{
               label: 'Sold',
               data: [
                 val[0].CategorySales,
                  val[1].CategorySales,
                  val[2].CategorySales,
                  val[3].CategorySales,
               ],
               borderWidth: 0.5,
               borderColor: '#fa2485',
                backgroundColor: [
                    'rgb(255, 99, 132, 0.2)',
                     'rgb(54, 162, 235, 0.2)',
                     'rgb(255, 225, 86, 0.5)',
                     'rgb(180, 137, 186, 0.5)',
                ],
               fill: true,
            }]
          },
          options: {
               pointStyle: false,
               tension: 0.4,
                 hoverOffset: 4,
            scales: {
              y: {
                title: {
                    display: false,
                    text: 'Sum'
                }
              },
              x: {
                  title: {
                      display: true,
                      text: "Category"
                  }
              }
            }
          }
        });
        // Category sales chart



        var jsonSales96 = '@Html.Raw(ViewData["Sales96"])';
        var value96 = JSON.parse(jsonSales96);

        var jsonSales97 = '@Html.Raw(ViewData["Sales97"])';
        var values97 = JSON.parse(jsonSales97);

        var jsonSales98 = '@Html.Raw(ViewData["Sales98"])';
        var values98 = JSON.parse(jsonSales98);

        var ctx = document.getElementById('multiline').getContext('2d');
        let delayed;
        var myChart = new Chart(ctx, {
            type: 'line',
            data: {
                labels: value96.map(m => m.CategoryName),
                datasets: [{
                    label: '1996',
                    data: value96.map(m => m.ProductSales),
                    borderColor: 'rgba(75, 192, 192, 1)',
                    borderWidth: 2,
                    hitRadius: 30,
                    fill: false
                },
                {
                    label: '1997',
                    data: values97.map(a => a.ProductSales),
                    borderColor: 'rgb(205, 102, 255)',
                    borderWidth: 2,
                    hitRadius: 30,
                    backgroundColor: 'rgba(153, 102, 255, 0.2)',
                    fill: false
                },
                {
                    label: '1998',
                    data: values98.map(z => z.ProductSales),
                    borderColor: 'rgba(255, 159, 132, 1)',
                    borderWidth: 2,
                    hitRadius: 30,
                    backgroundColor: 'rgba(255, 159, 132, 0.1)',
                    fill: false
                }]
            },
            options: {
                padding: 20,
                tension: 0.4,
                responsive: true,
                animation: {
                    tension: {
                        duration: 700,
                        easing: 'linear',
                        from: 0.7,
                        to: 0,
                        loop: true
                    },
                    onComplete: () => {
                        delayed = true;
                    },
                    delay: (context) => {
                        let delay = 0;
                        if (context.type === "data" && context.mode === "default" && !delayed) {
                            delay = context.dataIndex * 300 + context.datasetIndex * 100;
                        }
                        return delay;
                    },
                },
                scales: {
                    x: {
                        display: true,
                        title: {
                            display: true,
                            text: 'Products'
                        }
                    },
                    y:
                    {
                        ticks: {
                            callback: function (value) { return "$ " + Math.min(value) },
                        },
                        type: 'linear',
                        display: true,
                        title: {
                            display: true,
                            text: 'Value'
                        }
                    }
                }
            }
        });
    
    </script>


}


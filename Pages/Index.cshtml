@page
@model IndexModel

@{
    ViewData["Title"] = "Northwind-In-Chart";
}
<div class="container-fluid mx-auto" style="max-width: 90%;">

    <div class="row mt-4">

        @* Product Sales In Total *@
        <div class="col-md-3 mb-2">
            <div class="d-flex flex-row widget summary totalSum">
                <div class="d-flex flex-column p-2">
                    <i class="bi-currency-euro fs-3 text-success"></i>
                </div>
                <div class="flex-column m-auto">
                    <div class="text-success">@ViewData["ProductSalesInTotal"] 
                        <i class="bi-arrow-up text-success"></i>
                    </div>
                    <span class="fs-6">Total Category sales </span>
                </div>
            </div>
        </div>

        @* Shipper *@
        <div class="col-md-3 mb-2">
            <div class="d-flex flex-row widget summary totalQuantity">
                <div class="d-flex flex-column justify-content-center p-2">
                    <i class="bi-currency-euro fs-3 text-danger"></i>
                </div>
                <div class="flex-column m-auto">
                    <div class="text-danger">@ViewData["Shipper"]</div>
                    <span class="fs-6">Shipers total</span>
                </div>
            </div>
        </div>

        @* Revenue *@
        <div class="col-md-3 mb-2">
            <div class="d-flex flex-row widget summary revenue border border-0">
                <div class="d-flex flex-column justify-content-center p-2">
                    <i class="bi-currency-euro fs-3 text-success align-self-auto"></i>
                </div>
                <div class="flex-column m-auto ">
                    <span class="text-danger">category</span>
                    <div class="vrednost">@ViewData["category"]</div>
                </div>
            </div>
        </div>

        @* Revenue *@
        <div class="col-md-3 mb-2 ">
            <div class="d-flex flex-row widget summary revenue border border-0">
                <div class="d-flex flex-column justify-content-center p-2">
                    <i class="bi-currency-euro fs-3 text-secondary"></i>
                </div>
                <div class="flex-column m-auto">
                    <span class="text-danger">Revenue</span>
                    <div class="vrednost">@ViewBag.Revenue</div>
                </div>
            </div>
        </div>
    </div>

    <div class="row" >
    

        @* Multiline chart *@
        <div class="col-md-6">
            <div class="text-center">
                <div class="row">
                    <div class="chart-container">
                    <canvas id="multiline" class="my-1 p-1 widget shadow-dark"></canvas>
                    </div>          
                </div>
            </div>
        </div>

        @* Category Sales flor 97 *@
        <div class="col-md-3">
            <div class="text-center">
                <div class="row">
                    @* Category Sales for 97 *@             
                    <div class="chart-container">
                    <canvas id="CategorySales97" class="my-1 widget shadow-dark px-3"></canvas>
                    </div>
                </div>
            </div>
        </div>
               
        @* Category Sales flor 96 *@
        <div class="col-md-3">
            <div class="text-center">
                <div class="row">
                    @* Category Sales for 97 *@  
                    <div class="chart-container">
                        <canvas id="CategorySales96" class="my-1 widget shadow-dark px-2 py-2"></canvas>
                    </div>
                </div>
            </div>
        </div>
        @* style="box-shadow: 2px 4px 8px 2px rgba(0, 0, 0, 1), 0 6px 20px 0 rgba(0, 0, 0, 1) !important;" *@
      
        <div class="col" style="max-width: 400px;">
            <div class="text-center">
                <div class="mt-1">
                    @foreach (var item in Model.Employees)
                    {
                        <div class="widget mt-2 p-2 shadow-dark">

                            <div class="rounded-5 text-center d-flex d-inline">
                                <img class="rounded-5 me-md-3" style="width: 3em; height:3em;" src="data:image/webp;base64,@Convert.ToBase64String(@item.Photo)" title="@item.LastName" />
                                <div class="align-self-center offset-1 fs-6">
                                        @item.TitleOfCourtesy @item.FirstName @item.LastName,
                                        <div class="text-center small">@item.Title</div>
                                </div>
                            </div>
                                               
                        </div>   
                    }                      
                </div>
            </div>
        </div>
    </div>
</div>



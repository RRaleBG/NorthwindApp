    // ======== Multiline chart ================================================== //
        var jsonSales96 = '@Html.Raw(ViewData["Sales96"])';
        var value96 = JSON.parse(jsonSales96);

        var jsonSales97 = '@Html.Raw(ViewData["Sales97"])';
        var values97 = JSON.parse(jsonSales97);

        var jsonSales98 = '@Html.Raw(ViewData["Sales98"])';
        var values98 = JSON.parse(jsonSales98);

        var ctx = document.getElementById('multiline').getContext('2d');
        let delayed;
        const myChart = new Chart(ctx, {
            type: 'line',
            data: {
                labels: value96.map(m => m.CategoryName),
                datasets: [{
                    label: '1996',
                    data: value96.map(m => m.ProductSales),
                    borderColor: 'rgba(75, 192, 192, 1)',
                    borderWidth: 2,
                    hitRadius: 30,
                    fill: false
                },
                {
                    label: '1997',
                    data: values97.map(a => a.ProductSales),
                    borderColor: 'rgb(205, 102, 255)',
                    borderWidth: 2,
                    hitRadius: 30,
                    backgroundColor: 'rgba(153, 102, 255, 0.2)',
                    fill: false
                },
                {
                    label: '1998',
                    data: values98.map(z => z.ProductSales),
                    borderColor: 'rgba(255, 159, 132, 1)',
                    borderWidth: 2,
                    hitRadius: 30,
                    backgroundColor: 'rgba(255, 159, 132, 0.1)',
                    fill: false
                }]
            },
            options: 
            {
                maintainAspectRatio: true,
                devicePixelRatio: 1,
                plugins: {
                    title: {
                        display: false,
                        text: 'Rale'
                    },
                    subtitle: {
                        display: false,
                        text: "Rados",
                        position: 'right',
                        color: 'rgb(255, 159, 132)',
                    }, 
                    tooltip: {
                        intersect: false,
                        usePointStyle: true,
                        callbacks: {
                            labelColor: function(context) {
                                return {
                                    borderColor: 'rgb(0, 15, 0)',
                                    backgroundColor: 'rgb(150, 250, 240)',
                                    borderWidth: 2,
                                    borderDash: [5, 5],
                                    borderRadius: 4,
                                };
                            },
                            labelTextColor: function(context) {                       
                                return {
                                    pointStyle: 'triangle',
                                    rotation: 1
                                };
                            },
                        }
                    },
                },                            
                padding: 50,
                tension: 0.4,
                responsive: true,
                animation: {
                    tension: {
                        duration: 700,
                        easing: 'linear',
                        from: 0.7,
                        to: 0,
                        loop: true
                    },
                    onComplete: () => {
                        delayed = true;
                    },
                    delay: (context) => {
                        let delay = 0;
                        if (context.type === "data" && context.mode === "default" && !delayed) {
                            delay = context.dataIndex * 300 + context.datasetIndex * 100;
                        }
                        return delay;
                    },
                },
                scales: {
                    x: {
                        display: true,
                        title: {
                            display: false,
                            text: 'Products'
                        }
                    },
                    y:
                    {
                        ticks: {
                            callback: function (value) { return "$ " + Math.min(value) },
                        },
                        type: 'linear',
                        display: true,
                        title: {
                            display: false,
                            text: 'Value'
                        }
                    }
                }
            }
        });         

    // Category sales 97 chart




    </script>


